<h1>Listing photos</h1>

<table>
  <thead>
    <tr>
      <th>Low resolution</th>
      <th>Standard resolution</th>
      <th>Link</th>
      <th>Thumbnail</th>
      <th>Location</th>
      <th>Instagram</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.low_resolution %></td>
        <td><%= photo.standard_resolution %></td>
        <td><%= photo.link %></td>
        <td><%= photo.thumbnail %></td>
        <td><%= photo.location %></td>
        <td><%= photo.instagram_id %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
